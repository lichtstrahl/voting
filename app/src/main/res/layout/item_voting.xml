<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="@dimen/margin_list_item_v"
    android:layout_marginBottom="@dimen/margin_list_item_v"
    android:layout_marginStart="@dimen/margin_std"
    android:layout_marginEnd="@dimen/margin_std"
    app:cardCornerRadius="10dp">

    <TextView
        android:id="@+id/votingName"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/margin_std"
        android:textSize="18sp"
        tools:text="@tools:sample/cities"/>

</androidx.cardview.widget.CardView>